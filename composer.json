{
    "name": "leggett/auth0",
    "description": "Auth0 Authenticate object for CakePHP 3.x",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1",
	"auth0/auth0-php": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "keywords": [
        "cakephp",
        "oauth2",
        "auth0",
        "authentication",
        "auth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Gim√©nez",
            "homepage": "http://www.montcat.org/portfolio",
            "role": "Author"
        },
        {
            "name": "Daniel Cole",
            "homepage": "http://github.com/optii",
            "role": "Author"
        }
    ],
    "autoload": {
        "psr-4": {
            "Auth0\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Auth0\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
